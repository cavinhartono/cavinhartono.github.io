datasource db {
  provider  = "sqlite"
  url       = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id          Int     @id @default(autoincrement())
  slug        String
  title       String
  content     String?
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}